          "_name": "features2",
          "_customHTML": "<section class=\"features4\" group=\"Features\" plugins=\"amp-main\">\n    <!--  -->\n    <mbr-parameters>\n        <header>Size</header>\n        <input type=\"range\" inline title=\"Top:\" name=\"paddingTop\" min=\"0\" max=\"10\" step=\"1\" value=\"1\">\n        <input type=\"range\" inline title=\"Bottom:\" name=\"paddingBottom\" min=\"0\" max=\"10\" step=\"1\" value=\"0\">\n       \n        <!-- -->\n        <header>Show/Hide</header>\n        <input type=\"checkbox\" title=\"Title:\" name=\"showTitle\">\n        <input type=\"checkbox\" title=\"Subtitle:\" name=\"showSubtitle\">\n        <!-- -->\n        <header>Cards</header>\n        <input type=\"range\" inline title=\"Cards:\" name=\"amountCards\" min=\"1\" max=\"3\" step=\"1\" value=\"3\">\n        <input type=\"checkbox\" title=\"Button\" name=\"showButtons\" checked>\n        <input type=\"checkbox\" title=\"Title:\" name=\"showCardTitle\" checked>\n        <input type=\"checkbox\" title=\"Transparent BG:\" name=\"transparentBg\">\n        <input type=\"color\" title=\"Color:\" name=\"cardBg\" value=\"#fafafa\" condition=\"!transparentBg\">\n        <input type=\"checkbox\" title=\"Icon:\" name=\"showIcon\" checked>\n        <select title=\"Icon Align:\" name=\"alignIcon\" condition=\"showIcon\">\n            <option value=\"1\">Left</option>\n            <option value=\"2\" selected>Center</option>\n            <option value=\"3\">Right</option>\n        </select>\n        <input type=\"color\" title=\"Color Icon 1:\" name=\"colorIcon1\" value=\"#d50000\" condition=\"showIcon\">\n        <input type=\"color\" title=\"Color Icon 2:\" name=\"colorIcon2\" value=\"#1aa722\" condition=\"showIcon\">\n        <input type=\"color\" title=\"Color Icon 3:\" name=\"colorIcon3\" value=\"#FFD400\" condition=\"showIcon\">\n        \n        <input type=\"checkbox\" title=\"Border:\" name=\"borderCard\" checked>\n        <!--  -->\n        <header>Background</header>\n        <fieldset type=